{
  "name": "ai-ensemble-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/consensus-core",
    "packages/consensus-standard",
    "packages/consensus-elo",
    "packages/consensus-majority",
    "packages/consensus-council",
    "packages/app",
    "packages/component-library",
    "packages/shared-utils",
    "packages/e2e",
    "packages/eval"
  ],
  "scripts": {
    "dev": "npm run dev:free",
    "dev:free": "npm run dev:free --workspace=packages/app",
    "dev:mock": "npm run dev:mock --workspace=packages/app",
    "dev:components": "npm run storybook --workspace=packages/component-library",
    "analyze:fta": "node ./scripts/check-fta.mjs",
    "build": "npm run build --workspace=@ensemble-ai/consensus-core --workspace=@ensemble-ai/consensus-standard --workspace=@ensemble-ai/consensus-elo --workspace=@ensemble-ai/consensus-majority --workspace=@ensemble-ai/consensus-council --workspace=@ensemble-ai/shared-utils --workspace=@ensemble-ai/eval --workspace=@ai-ensemble/component-library --workspace=@ai-ensemble/app",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "test:e2e": "npm run test --workspace=packages/e2e",
    "test:unit": "npm run test:unit --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present",
    "prepare": "husky",
    "start": "npm run start --workspace=@ai-ensemble/app"
  },
  "devDependencies": {
    "fta-cli": "^3.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14"
  },
  "dependencies": {
    "@radix-ui/react-use-effect-event": "^0.0.2"
  }
}
