{
  "name": "@ensemble-ai/eval",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "ensemble-eval": "./bin/ensemble-eval.mjs"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "eval": "node --import tsx bin/ensemble-eval.mjs quick-eval",
    "eval:standard": "node --import tsx bin/ensemble-eval.mjs quick-eval --strategies standard",
    "eval:elo": "node --import tsx bin/ensemble-eval.mjs quick-eval --strategies elo",
    "eval:majority": "node --import tsx bin/ensemble-eval.mjs quick-eval --strategies majority",
    "eval:mock": "node --import tsx bin/ensemble-eval.mjs quick-eval --mode mock",
    "eval:ci": "node --import tsx bin/ensemble-eval.mjs quick-eval --strategies standard,elo,majority,council --sample 10 --baseline baselines/quick-eval.json",
    "eval:regenerate-baseline": "node --import tsx bin/ensemble-eval.mjs quick-eval --no-cache --baseline baselines/quick-eval.json"
  },
  "dependencies": {
    "@ensemble-ai/shared-utils": "*",
    "commander": "^13.1.0",
    "systeminformation": "^5.31.1",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@types/node": "^22.7.9",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  }
}
