{
  "name": "@ensemble-ai/shared-utils",
  "version": "1.0.0",
  "description": "Shared utilities for Ensemble AI (similarity, crypto, embeddings, streaming)",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./similarity": {
      "types": "./dist/similarity/index.d.ts",
      "import": "./dist/similarity/index.js"
    },
    "./security": {
      "types": "./dist/security/index.d.ts",
      "import": "./dist/security/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "similarity",
    "cosine-similarity",
    "embeddings",
    "crypto",
    "streaming",
    "utilities"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.7.9",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.39.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "agentkeepalive": "^4.6.0",
    "axios": "^1.7.7",
    "form-data-encoder": "^1.7.2",
    "formdata-node": "^4.4.1",
    "node-fetch": "^2.7.0",
    "openai": "^4.68.4"
  }
}
